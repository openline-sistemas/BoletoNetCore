<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
	  <Title>BoletoNetCore.Openline</Title>
	  <Version>1.3.0-beta</Version>
	  <AssemblyName>BoletoNetCore.Openline</AssemblyName>
	  <Description>BoletoNetCore.Openline</Description>
	  <PackageId>BoletoNetCore.Openline</PackageId>
	  <authors>BoletoNetCore, Openline Solutions</authors>
      <PackageReleaseNotes>Implementação inicial carteira banco inter</PackageReleaseNotes>
	  <Copyright>BoletoNetCore</Copyright>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <ApplicationIcon />
    <OutputType>Library</OutputType>
    <StartupObject />
    <IsPackable>true</IsPackable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <EmbeddedResource Include="Imagens\*" />
    <EmbeddedResource Include="BoletoImpressao\**\*.css" />
    <EmbeddedResource Include="BoletoImpressao\**\*.jpg" />
    <EmbeddedResource Include="BoletoImpressao\**\*.jpeg" />
    <EmbeddedResource Include="BoletoImpressao\**\*.html" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="BoletoImpressao\Parts\Pix.html" />
    <None Remove="Imagens\085.jpg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="SkiaSharp" Version="2.88.6" />
    <PackageReference Include="SkiaSharp.NativeAssets.Linux" Version="2.88.3" />
    <PackageReference Include="System.Net.Http.Json" Version="7.0.1" />
  </ItemGroup>


</Project>
